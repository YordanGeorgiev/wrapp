{
   "interpolation_comment" : "the declaration order matters, already declared vars are interpolated",
   "env" : {
      "git" : {
         "private_key_file" : "~/.ssh/id_rsa.ysg",
         "git_clone_url" : "git@github.com:YordanGeorgiev/wrapp.git"
      },
      "log" : {
         "PrintErrorMsgs" : 1,
         "PrintConsoleMsgs" : 1,
         "LogToFile" : 1,
         "LogDir" : "$ProductInstanceDir/dat/log",
         "PrintInfoMsgs" : 1,
         "PrintWarningMsgs" : 1,
         "LogFile" : "$LogDir/$ProductName.%YYYY%.%MM%.%DD%.log",
         "LogTimeToTextSeparator" : ":::",
         "TimeFormat" : "YYYY-MM-DD hh:mm:ss",
         "PrintDebugMsgs" : 1,
         "PrintTraceMsgs" : 1
      },
      "ENV_TYPE" : "dev",
      "ssh" : {
         "ssh_key_pair_file" : "~/.ssh/key-pair-wrapp-amzn.pem",
         "ssh_user" : "ubuntu",
         "ssh_server" : "host-name"
      }
   },
   "multi_lines_comment" : "to indicate new line use \\n, to indicate 3 spaces \\t"
}
